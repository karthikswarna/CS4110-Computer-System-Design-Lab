/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux
{
    IN a, b, sel;
    OUT out;

    PARTS:
    // out = (a NAND (NOT sel)) NAND (n NAND sel)
    Nand(a=sel, b=sel, out=notSel);
    Nand(a=a, b=notSel, out=out1);
    Nand(a=sel, b=b, out=out2); 
    Nand(a=out1, b=out2, out=out);
}
